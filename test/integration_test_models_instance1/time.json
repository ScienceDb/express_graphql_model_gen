{
    "model": "Time",
    "storageType": "cassandra",
    "attributes": {
      "time_id": "uuid",
      "event_id": "uuid",
      "year": "int",
      "month": "tinyint",
      "day": "tinyint",
      "hour": "tinyint",
      "minute": "tinyint"
