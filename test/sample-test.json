{
  "model" : "Person",
  "table" : "people",
  "attributes" : {
    "firstName" : "String",
    "lastName" : "String",
    "email" : "String"
  },
  "associations" : [
    {
      "type" : "hasMany",
      "target" : "Dog",
      "target_table" : "dogs",
      "as" : "dogs",
      "foreign_key" : {
            "personId" : "Int"
        }
    },

    {
      "type" : "belongsToMany",
      "target" : "Book",
      "target_table" : "books",
      "as" : "books",
      "cross_table" : "books_to_people",
      "source_key" : {
            "personId" : "Int"
        },
      "target_key" : {
          "dogId" : "Int"
      }
    }
  ]
}
