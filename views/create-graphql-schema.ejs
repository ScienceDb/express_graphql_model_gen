module.exports = `

  type <%- name -%> {
    <% for (var key in attributes) { %>
      <%= key %>: <%= attributes[key] -%>
    <% } %>
  }

  type Query {
    <%- namePl -%>: [<%- name -%>]
    read<%- name -%>(id: ID): <%- name %>
  }

  type Mutation {
    add<%- name -%>( <%- attributesStr -%> ): <%- name -%>!
    delete<%- name -%>(id: ID!): String!
    update<%- name -%>(<%- attributesStr -%>): <%- name %>
  }

`;
