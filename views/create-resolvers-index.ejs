<% for (let i=0; i < models.length; i++ ){ -%>
  var resolvers<%= models[i][0] -%> = require('./<%= models[i][0].toLowerCase()-%>');
<%}-%>

module.exports = {
  <% for (let i=0; i < models.length; i++ ){ -%>
    <%= models[i][1] -%>: resolvers<%= models[i][0]%>.<%= models[i][1] -%>,
    search<%= models[i][0] -%>: resolvers<%= models[i][0]%>.search<%= models[i][0] -%>,
    readOne<%= models[i][0] -%>: resolvers<%= models[i][0]%>.readOne<%= models[i][0] -%>,
    add<%= models[i][0] -%>: resolvers<%= models[i][0]%>.add<%= models[i][0] -%>,
    delete<%= models[i][0] -%>: resolvers<%= models[i][0]%>.delete<%= models[i][0] -%>,
    update<%= models[i][0] -%>: resolvers<%= models[i][0]%>.update<%= models[i][0] %>,
    
  <%}-%>
};
