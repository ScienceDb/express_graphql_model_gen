    {
      return resolvers.<%=associations_temp[i].target_pl%>({"field" : <%=associations_temp[i].targetKey%>, "value":{"value":this.id }, "operator": "equal"});
    }else{
      let filtered = resolvers.<%=associations_temp[i].target_pl%>(input);
      return filtered.filter( res => {if(res.<%=associations_temp[i].targetKey%> == this.id){return true;} } );
    }

  }
<%}-%>



